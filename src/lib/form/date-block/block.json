{
  "name": "date",
  "supports": {
    "editable": true,
    "required": true,
    "attachment": true,
    "description": true,
    "logic": false,
    "theme": true
  },
  "attributes": {
    "format": {
      "type": "string",
      "enum": ["MMDDYYYY", "DDMMYYYY", "YYYYMMDD"],
      "default": "DDMMYYYY"
    },
    "separator": {
      "type": "string",
      "enum": ["/", "-", "."],
      "default": "/"
    }
  }
}
